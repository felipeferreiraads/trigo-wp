.cards-enterprise
    display flex
    justify-content space-between
    margin-top: 100px
    > *:not(:last-child)
        margin-right: 90px

.card-enterprise
    figure
        display: flex
        justify-content: center
        margin-bottom: 30px
        img
            width: 100px
            height 100px
    h3
        font-size: 35px
        color: $color.primary
        text-align: center
        margin-bottom: 30px
    p
        line-height: 1.8
        text-align: center